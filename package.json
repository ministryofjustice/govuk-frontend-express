    "govuk-frontend": "^5.4.0",
    "helmet": "^7.1.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nunjucks": "^3.2.4",
    "sass": "^1.77.4"
  },
  "devDependencies": {
    "nodemon": "^3.1.3"
  }
}
